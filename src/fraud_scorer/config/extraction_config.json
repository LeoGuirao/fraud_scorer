{
  "fields": {
    "numero_poliza": {
      "patterns": [
        "Póliza\\s*Número\\s*([\\d\\s\\-]+)",
        "Contrato\\s*No\\.\\s*([\\d\\s\\-]+)"
      ],
      "anchor_words": [
        "número de contrato",
        "folio de póliza"
      ]
    },
    "nombre_asegurado": {
      "patterns": [
        "Titular\\s*:\\s*([^\\n]+)",
        "Beneficiario\\s*:\\s*([^\\n]+)"
      ],
      "anchor_words": [
        "titular de la póliza",
        "beneficiario principal"
      ]
    },
    "custom_field": {
      "patterns": [
        "Campo\\s*Personalizado\\s*:\\s*([^\\n]+)"
      ],
      "anchor_words": [
        "campo custom",
        "dato adicional"
      ],
      "validators": [],
      "confidence_threshold": 0.5
    }
  },
  "extraction_settings": {
    "enable_llm_fallback": false,
    "enable_ml_context": false,
    "max_retries": 3,
    "cache_ttl": 3600
  }
}